		// TODO Auto-generated method stub
		int x;
		x = 1;
		switch(x){
			case 0:
			{
				System.out.println("Esta es la opcion 0");
				break;
			}
			case 1:
			{
				System.out.println("Esta es la opcion1");
				break;
			}
		
		}
	}

